fun main() {
    // Test basic math functions
    println("Testing math functions:")
    
    var x = 2.0
    var y = 3.0
    
    println("abs(-5.0) = " + abs(-5.0).toString())
    println("min(2.0, 3.0) = " + min(x, y).toString())
    println("max(2.0, 3.0) = " + max(x, y).toString())
    println("sqrt(16.0) = " + sqrt(16.0).toString())
    println("pow(2.0, 3.0) = " + pow(x, y).toString())
    println("sin(0.0) = " + sin(0.0).toString())
    println("cos(0.0) = " + cos(0.0).toString())
    println("tan(0.0) = " + tan(0.0).toString())
    
    // Test with integers
    println("abs(-10) = " + abs(-10).toString())
    println("min(5, 7) = " + min(5, 7).toString())
    println("max(5, 7) = " + max(5, 7).toString())
    
    // Test mixed types
    println("min(2.5, 3) = " + min(2.5, 3.0).toString())
    println("max(2, 3.5) = " + max(2.0, 3.5).toString())
    
    println("Math functions test completed!")
}